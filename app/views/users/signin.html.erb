
<h1> Enter SignIn Credentials </h1>

<%=form_for :user, :url => signin_path do |f| %>
  <p>
    <%= f.label :email%>
    <%= f.text_field :email%>
  </p> 

  <p>
    <%= f.label :password%>
    <%= f.password_field :password_digest%>
  </p>

  <%= f.submit "Sign In"%>

  <br/><br/><br/>

  <%= link_to "Sign UP" , user_signup_path%>
  
<% end %>